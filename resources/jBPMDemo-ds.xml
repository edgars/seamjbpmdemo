<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE datasources
    PUBLIC "-//JBoss//DTD JBOSS JCA Config 1.5//EN"
    "http://www.jboss.org/j2ee/dtd/jboss-ds_1_5.dtd">
<datasources>

	<local-tx-datasource>
		<jndi-name>jBPMDemoDatasource</jndi-name>
		<use-java-context>true</use-java-context>
		<connection-url>jdbc:postgresql:jbpmdemo</connection-url>
		<driver-class>org.postgresql.Driver</driver-class>
		<user-name>jbpmdemo</user-name>
		<password>jbpmdemo</password>
	</local-tx-datasource>

</datasources>
